{
  "name": "@onhand/framework-aws",
  "version": "2.0.8",
  "description": "framework-aws is part of the @onhand framework",
  "author": "Codeco <adrielcodeco@hotmail.com>",
  "homepage": "https://github.com/adrielcodeco/onhand/tree/main/packages/framework-aws#readme",
  "license": "ISC",
  "directories": {
    "lib": "#"
  },
  "files": [
    "#"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adrielcodeco/onhand.git"
  },
  "scripts": {
    "build": "yarn clean && tsc -b tsconfig.build.json",
    "clean": "rimraf '#' coverage yarn*.log",
    "test": "jest --coverage --passWithNoTests",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "yarn:pack": "yarn pack && cpy 'onhand-*.tgz' '../../releases'"
  },
  "bugs": {
    "url": "https://github.com/adrielcodeco/onhand/issues"
  },
  "dependencies": {
    "@onhand/business": "^2.0.8",
    "@onhand/business-aws": "^2.0.8",
    "@onhand/controller": "^2.0.8",
    "@onhand/framework": "^2.0.8",
    "@onhand/openapi": "^2.0.8",
    "@onhand/utils": "^2.0.8",
    "accesscontrol": "^2.2.1",
    "amazon-cognito-identity-js": "^5.2.3",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1042.0",
    "class-validator": "^0.13.2",
    "cross-fetch": "^3.0.6",
    "dynamoose": "^2.8.3",
    "inversify": "^6.0.1",
    "inversify-binding-decorators": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.4",
    "lodash": "^4.17.21",
    "node-cache": "^5.1.2",
    "pify": "^5.0.0",
    "reflect-metadata": "^0.1.13",
    "winston": "^3.3.3"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "isolatedModules": true
      }
    },
    "moduleNameMapper": {
      "^#/(.*)$": "<rootDir>/src/$1",
      "^$/(.*)$": "<rootDir>/tests/$1"
    },
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "./tests/.+.ts$",
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.{js,ts}"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "coverageReporters": [
      "lcov"
    ]
  }
}
