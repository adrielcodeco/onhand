{
  "name": "@onhand/robot",
  "version": "2.0.6",
  "description": "robot is part of the @onhand framework",
  "author": "Codeco <adrielcodeco@hotmail.com>",
  "homepage": "https://github.com/adrielcodeco/onhand/tree/main/packages/robot#readme",
  "license": "ISC",
  "directories": {
    "lib": "#"
  },
  "files": [
    "#",
    "public",
    "templates"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adrielcodeco/onhand.git"
  },
  "scripts": {
    "build": "yarn clean && tsc -b tsconfig.build.json",
    "client:build": "parcel build src/client/index.html --out-dir public",
    "client:watch": "parcel watch src/client/index.html --out-dir public",
    "clean": "rimraf '#' coverage yarn*.log",
    "test": "jest --coverage --passWithNoTests",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "yarn:pack": "yarn pack && cpy 'onhand-*.tgz' '../../releases'",
    "dev": "nodemon -w \"src/**/*\" -i \"src/client/**/*\" -e \"*\" --exec \"yarn build\""
  },
  "bugs": {
    "url": "https://github.com/adrielcodeco/onhand/issues"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "isolatedModules": true
      }
    },
    "moduleNameMapper": {
      "^#/(.*)$": "<rootDir>/src/$1",
      "^$/(.*)$": "<rootDir>/tests/$1"
    },
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "./tests/.+.ts$",
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.{js,ts}"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "coverageReporters": [
      "lcov"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.44.0",
    "@aws-sdk/shared-ini-file-loader": "^3.37.0",
    "@babel/core": "^7.16.0",
    "@material-ui/core": "^4.12.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/styles": "^4.11.4",
    "@mdi/js": "^6.5.95",
    "@onhand/business": "^2.0.6",
    "@onhand/framework": "^2.0.6",
    "@onhand/iac-aws": "^2.0.6",
    "@onhand/openapi": "^2.0.6",
    "@onhand/utils": "^2.0.6",
    "crypto-js": "^4.1.1",
    "execa": "5.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "func-loc": "^0.1.16",
    "helmet": "^4.6.0",
    "memorystore": "^1.6.6",
    "morgan": "^1.10.0",
    "nconf": "^0.11.3",
    "notistack": "^2.0.3",
    "parcel-plugin-nuke-dist": "^1.0.1",
    "platform-folders": "^0.5.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.2",
    "short-uuid": "^4.2.0",
    "yeoman-environment": "^3.8.1"
  },
  "devDependencies": {
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11"
  }
}
