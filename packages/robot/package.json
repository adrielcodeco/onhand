{
  "name": "@onhand/robot",
  "version": "1.0.56",
  "description": "robot is part of the @onhand framework",
  "author": "Codeco <adrielcodeco@hotmail.com>",
  "homepage": "https://github.com/adrielcodeco/onhand/tree/main/packages/robot#readme",
  "license": "ISC",
  "directories": {
    "lib": "#"
  },
  "files": [
    "#"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adrielcodeco/onhand.git"
  },
  "scripts": {
    "build": "yarn clean && ttsc -b tsconfig.build.json",
    "client:build": "parcel build src/client/index.html --out-dir public",
    "client:watch": "parcel watch src/client/index.html --out-dir public",
    "clean": "rimraf '#' coverage yarn*.log",
    "test": "jest --coverage --passWithNoTests",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "yarn:pack": "yarn pack && cpy 'onhand-robot-*.tgz' '../../releases'",
    "dev": "nodemon -w src -e \"*\" --exec \"yarn build\""
  },
  "bugs": {
    "url": "https://github.com/adrielcodeco/onhand/issues"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "isolatedModules": true
      }
    },
    "moduleNameMapper": {
      "^#/(.*)$": "<rootDir>/src/$1",
      "^$/(.*)$": "<rootDir>/tests/$1"
    },
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "./tests/.+.ts$",
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.{js,ts}"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "coverageReporters": [
      "lcov"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.14.6",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/styles": "^4.11.4",
    "@mdi/js": "^5.9.55",
    "@onhand/utils": "^1.0.56",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "helmet": "^4.6.0",
    "memorystore": "^1.6.6",
    "morgan": "^1.10.0",
    "nconf": "^0.11.3",
    "notistack": "^1.0.9",
    "parcel-plugin-nuke-dist": "^1.0.1",
    "platform-folders": "^0.5.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "short-uuid": "^4.2.0",
    "yeoman-environment": "^3.5.1",
    "yeoman-generator": "^5.4.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.5"
  }
}
